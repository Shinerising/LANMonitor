<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib"
                    xmlns:local="clr-namespace:LanMonitor">

    <system:String x:Key="Title">网络监控</system:String>
    <system:String x:Key="UploadSpeed">上传速度</system:String>
    <system:String x:Key="DownloadSpeed">下载速度</system:String>
    <system:String x:Key="UnknownIP">不可用</system:String>
    <system:String x:Key="ComputerToolTip">计算机名称：{1}{0}IP地址：{2}{0}网络延迟：{3}</system:String>
    <system:String x:Key="NetworkToolTip">网卡名称：{1}{0}IP地址：{2}{0}MAC地址：{3}{0}带宽：{4}</system:String>
    
</ResourceDictionary>