<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib"
                    xmlns:local="clr-namespace:LanMonitor">

    <system:String x:Key="Title">LAN Monitor</system:String>
    <system:String x:Key="UploadSpeed">Upload Speed</system:String>
    <system:String x:Key="DownloadSpeed">Download Speed</system:String>
    <system:String x:Key="UnknownIP">Unknown</system:String>
    <system:String x:Key="ComputerToolTip">Computer Name: {1}{0}IP Address: {2}{0}Network Latency: {3}</system:String>
    <system:String x:Key="NetworkToolTip">Network Adapter: {1}{0}IP Address: {2}{0}Mac Address: {3}{0}Bandwidth: {4}</system:String>

</ResourceDictionary>